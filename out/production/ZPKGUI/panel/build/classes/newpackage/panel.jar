public int getFigura(){
    return Figura;
}

public void setFigura(int value){
    Figura=value;
}
//to mialo byc do znikania jtextfield, ktore nie beda uzywane do aktualnego liczenia, ale poki co nie dziala
public void Zmiana(){
    if(getFigura()==1){                 //okrag
        jTextField1.setEnabled(true);
        jTextField1.setVisible(true);
        jTextField2.setEnabled(false);
        jTextField2.setVisible(false);
        jTextField3.setEnabled(false);
        jTextField3.setVisible(false);
        jTextField4.setEnabled(false);
        jTextField4.setVisible(false);
    }else if(getFigura()==0){           //prostokat       
        jTextField1.setEnabled(false);
        jTextField1.setVisible(false);
        jTextField2.setEnabled(true);
        jTextField2.setVisible(true);
        jTextField3.setEnabled(true);
        jTextField3.setVisible(true);
        jTextField4.setEnabled(false);
        jTextField4.setVisible(false);
    }else if(getFigura()==2){           //trapez
        jTextField1.setEnabled(false);
        jTextField1.setVisible(false);
        jTextField2.setEnabled(true);
        jTextField2.setVisible(true);
        jTextField3.setEnabled(true);
        jTextField3.setVisible(true);
        jTextField4.setEnabled(true);
        jTextField4.setVisible(true);
    }else if(getFigura()==3){           //romb
        jTextField1.setEnabled(false);
        jTextField1.setVisible(false);
        jTextField2.setEnabled(true);
        jTextField2.setVisible(true);
        jTextField3.setEnabled(true);
        jTextField3.setVisible(true);
        jTextField4.setEnabled(false);
        jTextField4.setVisible(false);
    }else if(getFigura()==4){           //rownoleglobok
        jTextField1.setEnabled(false);
        jTextField1.setVisible(false);
        jTextField2.setEnabled(true);
        jTextField2.setVisible(true);
        jTextField3.setEnabled(true);
        jTextField3.setVisible(true);
        jTextField4.setEnabled(false);
        jTextField4.setVisible(false);
    }
